{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "jest-dom"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "react-app",
        "react-app/jest",
        "plugin:jest-dom/recommended",
        "prettier"
    ],
    "rules": {
        "jsx-a11y/anchor-has-content": 0,
        "jsx-a11y/alt-text": 0
    }
}
